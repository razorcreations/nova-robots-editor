{
    "name": "razorcreations/nova-robots-editor",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "laravel/nova": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Razorcreations\\NovaRobotsEditor\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Razorcreations\\NovaRobotsEditor\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
